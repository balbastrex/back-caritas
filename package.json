{
  "name": "apicaritas",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node dist/index.js",
    "build": "tsc && copyfiles ./resources/* dist/",
    "dev": "tsc-watch --onSuccess \"ts-node src/index.ts\" ",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -c default",
    "migrate:generate": "npm run typeorm migration:generate -- -n",
    "migrate:create": "npm run typeorm migration:create -- -n",
    "migrate:run": "npm run typeorm migration:run",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n ormconfig.js"
  },
  "author": "Vicente Albert",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.2",
    "reflect-metadata": "^0.1.10",
    "tslib": "^2.3.1",
    "typeorm": "0.2.41",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^8.0.29",
    "ts-node": "8.2.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.3.5"
  }
}
